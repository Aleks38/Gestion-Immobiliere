<Page x:Class="WPF_Exo.Views.Details.Modify.BoxDoModify"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WPF_Exo.Views.Details.Modify"
      mc:Ignorable="d" 
      d:DesignHeight="133" d:DesignWidth="400"
      Title="BoxDoModify">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>

        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        
        <Frame x:Name="FrmBoxModify" Grid.RowSpan="2" Navigated="FrmBoxModify_Navigated" />
        <Button Content="Modifier" Grid.Row="2"  HorizontalAlignment="Center" VerticalAlignment="Center" Click="btnModifier"/>
    </Grid>
</Page>
